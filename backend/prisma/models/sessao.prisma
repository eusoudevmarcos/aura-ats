model Sessao {
    id        Int      @id @default(autoincrement())
    token     String   @unique
    expiresAt DateTime

    usuarioSistemaId String         @unique
    usuarioSistema   UsuarioSistema @relation(fields: [usuarioSistemaId], references: [id])

    @@index([usuarioSistemaId])
    @@map("sessao")
    @@schema("Sessao")
}
